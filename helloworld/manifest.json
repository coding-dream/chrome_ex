{
	// 清单文件的版本，这个必须写，而且必须是2
    "manifest_version": 2,
	// 插件的名称
    "name": "Http模拟请求(跨域Ajax)",
	// 插件的版本
    "version": "1.0",
	// 插件的描述
    "description": "Http模拟请求",
	// 图标，一般偷懒全部用一个尺寸的也没问题
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
	// browser_action、page_action、app必须三选一
	 "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "Ajax跨域请求",
        "default_popup": "popup.html"
    },
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
    "permissions": [
        "http://www.java1234.com/",
		"http://www.mm131.com/xinggan/"
    ]
}